{
    "compilerOptions": {
          "esModuleInterop": true,
          "skipLibCheck": true,
          "forceConsistentCasingInFileNames": true,
          "resolveJsonModule": true,
      "outDir": "./dist",
          "strict": true,
          "target": "ES2022",
      "rootDir": "./src",
      "moduleResolution": "NodeNext",
      "module": "NodeNext"
    },
    "include": [
      "src/**/*.ts"
    ],
        "exclude": ["node_modules"]
  }